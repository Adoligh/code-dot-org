.key
  %dl
    %dt
      %span.puzzle_outer_level
        %a.level_link{ href: '', class: 'not_tried', onclick: 'return false;'}
          %span.puzzle-number 1
    %dd= t('progress.not_completed')
    %dt
      %span.puzzle_outer_level
        %a.level_link{ href: '', class: 'passed', onclick: 'return false;'}
          = check_mark_html
    %dd= t('progress.completed_too_many_blocks')
    %dt
      %span.puzzle_outer_level
        %a.level_link{ href: '', class: 'perfect', onclick: 'return false'}
          = check_mark_html
    %dd= t('progress.completed_perfect')
